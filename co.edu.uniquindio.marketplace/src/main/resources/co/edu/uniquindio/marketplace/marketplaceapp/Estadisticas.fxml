<?xml version="1.0" encoding="UTF-8"?>



<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.Button?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="co.edu.uniquindio.marketplace.marketplaceapp.controller.EstadisticasController">
    <children>
        <Label text="Panel de Estadísticas" layoutX="20" layoutY="20"/>
        <TextArea fx:id="estadisticasArea" layoutX="20" layoutY="50" prefWidth="400" prefHeight="200"/>
        <Button text="Exportar Estadísticas" layoutX="20" layoutY="270" onAction="#handleExportar"/>
    </children>
</AnchorPane>
